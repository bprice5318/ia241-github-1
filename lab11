        if max_p< int(v1):
            max_p = int(v1)
            max_county = name
total = max_p

url_str = 'https://api.census.gov/data/2016/acs/acs5?get=B01001_002E,NAME&for=county:*&in=state:51&key='+census_api_key # create the url of your census data
