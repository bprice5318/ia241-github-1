html_str = response.read().decode("utf-8") # convert the response into string
max_men = 0
max_county = ''
if (html_str):
    json_data = json.loads(html_str) # convert the string into json
    for v1,name,state,county in json_data[1:]:
        if max_men< int(v1):
            max_men = int(v1)
            max_county = name
print(max_men, ':', max_p)
